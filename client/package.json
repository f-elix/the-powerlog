{
	"name": "svelte-app",
	"version": "1.0.0",
	"scripts": {
		"dev": "run-p routify rollup",
		"dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
		"build": "routify -bD && rollup -c",
		"serve": "spassr --serve-spa --serve-ssr",
		"export": "routify export",
		"deploy:now": "cd scripts/now && npm run deploy",
		"deploy:netlify": "cd scripts/netlify && npm run deploy",
		"rollup": "rollup -cw",
		"routify": "routify -D"
	},
	"devDependencies": {
		"cross-env": "^7.0.2",
		"@rollup/plugin-alias": "^3.1.0",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^2.3.2",
		"concurrently": "^5.1.0",
		"del": "^5.1.0",
		"dotenv": "^8.2.0",
		"npm-run-all": "^4.1.5",
		"ppid-changed": "^1.0.1",
		"rollup": "^2.6.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-livereload": "^1.2.0",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-svelte": "^5.2.1",
		"rollup-plugin-terser": "^5.3.0",
		"spassr": "^1.0.2"
	},
	"dependencies": {
		"@sveltech/routify": "^1.7.10",
		"bson-objectid": "^1.3.0",
		"register-service-worker": "^1.7.1",
		"svelte": "^3.22.2",
		"validator": "^13.0.0",
		"xstate": "^4.9.1"
	}
}
